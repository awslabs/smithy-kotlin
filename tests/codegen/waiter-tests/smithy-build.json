{
  "version": "1.0",
  "projections": {
    "waiter-tests": {
      "transforms": [
        {
          "name": "includeServices",
          "args": {
            "services": [
              "com.test#WaitersTestService"
            ]
          }
        }
      ],
      "plugins": {
        "kotlin-codegen": {
          "service": "com.test#WaitersTestService",
          "package": {
